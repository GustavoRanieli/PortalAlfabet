        <option value="4">4ª Série</option>
    </select>
    
    <% for(i=0; i < arrayDataUser[0].cruzadas.length; i++){ %>
    
        <div style="display: block;" id="quiz-button" class="quiz-button">
            <a href='cruzadas/<%= arrayDataUser[0].id %>/<%= i %>' class="link-quiz">
